{
  "name": "@ajperezbau/vue-msw-devtools",
  "version": "0.1.0",
  "description": "A powerful browser-based UI for managing Mock Service Worker (MSW) handlers in Vue 3 applications.",
  "type": "module",
  "author": "ajperezbau",
  "license": "MIT",
  "keywords": [
    "msw",
    "devtools",
    "mock",
    "vue",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ajperezbau/vue-msw-devtools"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/msw-devtools.umd.cjs",
  "module": "./dist/msw-devtools.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/msw-devtools.js",
      "require": "./dist/msw-devtools.umd.cjs"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "playwright test --reporter=list",
    "test:ui": "playwright test --ui"
  },
  "peerDependencies": {
    "msw": "^2.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.58.0",
    "@types/node": "24.10.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.8.1",
    "msw": "2.12.10",
    "typescript": "~5.9.3",
    "vite": "7.2.4",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vite-plugin-dts": "4.5.4",
    "vue": "3.5.24",
    "vue-tsc": "3.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "msw"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
